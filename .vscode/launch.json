{
    "version": "0.2.0",
    "configurations": 
    [
        {
            "name": "GD32F30x",
            "cwd": "${workspaceRoot}",
            "executable": "build/GDprimeS3.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "preLaunchTask": "build",
            "gdbPath": "arm-none-eabi-gdb",
            "svdFile": "${workspaceRoot}/GD32F30x_HD.svd",
