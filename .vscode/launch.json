            {
                "enabled": true,
                "swoFrequency": 480000,
                "cpuFrequency": 24000000,
                "decoders": 
                [
                    {
                        "label": "RTT channel 0",
                        "port": 0,
                        "timestamp": true,
                        "type": "console"
                    }
                ]
            }
        }
   ]
